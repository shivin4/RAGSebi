# LlamaParser API Keys Configuration
# Copy this file to .env and fill in your actual API keys
# You need at least one API key, but can provide up to 4 for rotation

LLAMA_PARSE_API_KEY_1=your_first_api_key_here
LLAMA_PARSE_API_KEY_2=your_second_api_key_here
LLAMA_PARSE_API_KEY_3=your_third_api_key_here
LLAMA_PARSE_API_KEY_4=your_fourth_api_key_here

# Optional: Configure parsing settings
LLAMA_PARSE_RESULT_TYPE=markdown  # Options: markdown, text
LLAMA_PARSE_LANGUAGE=en
LLAMA_PARSE_PARSING_INSTRUCTION=Extract all text content including tables, maintaining document structure
